#include<bits/stdc++.h>
using namespace std;
int sumofdigits(int num){
    int sum = 0;
    int lastdigit;
    while(num>0){
        int lastdigit=num%10;
        num=num/10;
        sum=sum+lastdigit;
    }
    return sum;
    

}

int main(){
    int a ;
    cin>>a;
    cout<<sumofdigits(a)<<endl;
}
